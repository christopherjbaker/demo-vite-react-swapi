    "eslint-plugin-react-hooks": "5.2.0-canary-0a82580b-20250203",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.23.0",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
